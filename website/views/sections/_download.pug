        ul( class='list-inline banner-social-buttons' )
          li
            a( href='dist/roostr.iife.min.js' class='btn btn-default btn-lg' target='_blank' download='roostr.iife.min.js' title='Minified library only')=t('section.download.min')
          li
            a( href='https://github.com/LCluber/RoostR.js' class='btn btn-default btn-lg' target='_blank' title='Sources for the entire project')
              i( class='fa fa-github fa-fw' )
              =t('section.download.github')
